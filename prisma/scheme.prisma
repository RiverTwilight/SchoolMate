generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = "file:./db.sqlite"
}

model Student {
    id        String   @id @default(uuid())
    createdAt DateTime @default(now())

    name   String
    gender String
    class  Int
    grade  Int
    school String
    token  String?

    active Boolean @default(false)
}

model MusicVoteSession {
    id String @id @default(uuid())

    deadline DateTime @default(now())
}

model Musics {
    id String @id @default(uuid())

    vote   Int
    reason String
    lyrics String
}
