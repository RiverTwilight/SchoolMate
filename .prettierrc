{
    "trailingComma": "es5",
    "tabWidth": 4,
    "importOrder": [
        "^@core/(.*)$",
        "^@server/(.*)$",
        "^@ui/(.*)$",
        "^[./]",
        "^[../]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "semi": false,
    "singleQuote": true
}
